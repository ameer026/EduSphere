# Stop all Streamlit processes
Get-Process -Name "streamlit" -ErrorAction SilentlyContinue | Stop-Process -Force

# Remove the .streamlit folder and pages
Remove-Item ".streamlit" -Recurse -Force -ErrorAction SilentlyContinue
Remove-Item "pages" -Recurse -Force -ErrorAction SilentlyContinue

# Create fresh pages directory
New-Item -ItemType Directory -Path "pages" -Force
